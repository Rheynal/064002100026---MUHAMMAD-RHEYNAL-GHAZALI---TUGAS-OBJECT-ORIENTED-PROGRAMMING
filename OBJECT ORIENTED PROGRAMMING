print()
class kendaraan():
    def _init_(self):
        print()
        print('jenis kendaraan darat')

class mobil(kendaraan): 
    def __init__(self,merk,model):
        super()._init_()
        self.merk = str.capitalize(merk)
        self.model = str.capitalize(model)
        print(f'{self.merk} {self.model} adalah kendaraan yang beroda empat')
        

class bus(kendaraan):
    def __init__(self,merk,model):
        super()._init_()
        self.merk = str.capitalize(merk)
        self.model = str.capitalize(model)
        print(f'{self.merk} {self.model} adalah kendaraan yang beroda empat atau lebih')

class sepeda(kendaraan):
    def __init__(self,merk,model):
        super()._init_()
        self.merk = str.capitalize(merk)
        self.model = str.capitalize(model)
        print(f'{self.merk} {self.model} adalah kendaraan yang beroda dua')

class motor(kendaraan):
    def __init__(self,merk,model):
        super()._init_()
        self.merk = str.capitalize(merk)
        self.model = str.capitalize(model)
        print(f'{self.merk} {self.model} adalah kendaraan yang beroda dua')

x1 = mobil('civic','rush')
x2 = bus('malang times','bus pariwisata')
x3 = sepeda('polygon','wimcycle')
x4 = motor('ducati','vario')
